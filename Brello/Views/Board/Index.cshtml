@model IEnumerable<string>

@{

        Layout = "~/Views/Shared/_BrelloLayout.cshtml";
}
<div id="board" class="green container-fluid">
    <div class="row" style="display:inline-flex;">
        @for (int i = 0; i < 10; i++)
        {
            <div class="brellolist-wrapper">
                <div class="brellolist-header"></div>
                <div class="brellolist">
                    <ul class="center">
                        @foreach (var item in Model)
                        {
                            <li style="list-style:none;" class="text-center"><a href="https://google.com">@item</a></li>
                        }
                    </ul>
                </div>
                <div class="brellolist-footer"></div>
            </div>
        }
    </div>
</div>






